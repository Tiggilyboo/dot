! urxvt
URxvt.depth: 32
URxvt*borderless: 1
URxvt.iso14755: false
URxvt.font: xft:Hack:antialias=true:hinting=true:size=9:style=regular
URxvt.boldfont: xft:Hack:antialias=true:hinting=true:size=9:style=bold
URxvt.italicfont: xft:Hack:antialias=true:hinting=true:size=9:style=italic
urxvt*boldMode:      on
urxvt*colorMode:     on
urxvt*colorBDMode:   on
urxvt*colorULMode:   on
urxvt*underLine:     on
URxvt.scrollStyle: rxvt
URxvt.scrollBar: false
URxvt.scrollBar_right: false
URxvt.scrollBar_floating: false
Urxvt*secondaryScroll:  true
URxvt.letterSpace: -1
URxvt.lineSpace: 0
URxvt.fading: 0
URxvt*buffered: true
URxvt*loginShell: true
urxvt*transparent: true
urxvt*shading: 10
urxvt*tintColor: #000000
urxvt*blurRadius: 5

*background: #1a1a1a
*foreground: #c7c7c7
*color0: #000000
